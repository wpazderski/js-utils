{
    "name": "@wpazderski/js-utils",
    "version": "1.0.0",
    "description": "Common JavaScript utils",
    "keywords": [
        "js",
        "javascript",
        "ts",
        "typescript",
        "web",
        "node",
        "utils"
    ],
    "author": "Witold Pazderski",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/wpazderski/js-utils.git"
    },
    "homepage": "https://github.com/wpazderski/js-utils",
    "bugs": {
        "url": "https://github.com/wpazderski/js-utils/issues"
    },
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/common/index.d.ts",
            "default": "./dist/common/index.js"
        },
        "./node": {
            "types": "./dist/node/index.d.ts",
            "default": "./dist/node/index.js"
        },
        "./web": {
            "types": "./dist/web/index.d.ts",
            "default": "./dist/web/index.js"
        },
        "./*": "./dist/*"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "rimraf ./dist/ && rimraf -g ./node_modules/.tmp/*.tsbuildinfo && tsc -b",
        "build-docs": "node ./scripts/build-docs.ts",
        "lint-check": "eslint ./ --flag unstable_native_nodejs_ts_config --flag unstable_config_lookup_from_file --max-warnings 0",
        "lint-fix": "eslint ./ --flag unstable_native_nodejs_ts_config --flag unstable_config_lookup_from_file --max-warnings 0 --fix",
        "prettier-check": "prettier ./ --check",
        "prettier-fix": "prettier ./ --write",
        "tsc-check": "tsc -b --noEmit",
        "check-licenses": "check-licenses",
        "check-outdated-packages": "check-outdated-packages",
        "check-all": "run-s lint-check prettier-check tsc-check check-licenses check-outdated-packages",
        "pre-commit-checks": "run-s check-all",
        "dev": "vite --config devServer/vite.config.ts --port 7101",
        "start-test-server": "cross-env JS_UTILS_IS_TEST_ENV=true vite --config devServer/vite.config.ts --host --port 7101",
        "test-chromium": "playwright test --project=chromium",
        "test-firefox": "playwright test --project=firefox",
        "test-webkit": "playwright test --project=webkit",
        "test": "playwright test --project=chromium",
        "test-all": "playwright test"
    },
    "devDependencies": {
        "@playwright/test": "^1.54.2",
        "@types/node": "^24.2.1",
        "@types/web": "^0.0.261",
        "@wpazderski/eslint-config": "^1.0.5",
        "@wpazderski/node-package-tools": "^1.0.0",
        "@wpazderski/playwright-config": "^1.0.4",
        "@wpazderski/playwright-utils": "^1.0.2",
        "@wpazderski/prettier-config": "^1.0.3",
        "@wpazderski/typescript-config": "^1.0.4",
        "cross-env": "^10.0.0",
        "eslint": "^9.33.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.6.2",
        "rimraf": "^6.0.1",
        "typedoc": "^0.28.10",
        "typedoc-github-theme": "^0.3.0",
        "typescript": "^5.9.2",
        "vite": "^7.1.2"
    },
    "packageManager": "pnpm@10.11.0",
    "engines": {
        "node": ">=24.0.0",
        "pnpm": ">=10.0.0",
        "npm": "please-use-pnpm",
        "yarn": "please-use-pnpm"
    }
}
